	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		String arrayIndex[] = TEXTO.split("");
		Integer posicionMayorque;
		
		
		
		posicionMayorque = TEXTO.indexOf(">");
		
		
		
		
		
	}

}
